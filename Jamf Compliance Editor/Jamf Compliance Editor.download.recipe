<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Comment</key>
	<string>Created with Recipe Robot v1.1.2 (https://github.com/homebysix/recipe-robot)</string>
	<key>Description</key>
	<string>Downloads the latest version of Jamf Compliance Editor.</string>
	<key>Identifier</key>
	<string>com.github.jelockwood.recipes.download.JamfComplianceEditor</string>
	<key>Input</key>
	<dict>
		<key>NAME</key>
		<string>Jamf Compliance Editor</string>
        </dict>
        <key>MinimumVersion</key>
        <string>0.2.0</string>
	<key>Process</key>
	<array>
		<dict>
			<key>Processor</key>
			<string>URLTextSearcher</string>
			<key>Arguments</key>
			<dict>
				<key>re_pattern</key>
				<string>(\/\/cdn\.document360\.io\/\S{8}-\S{4}-\S{4}-\S{4}-\S{12}\/Images\/Documentation\/Jamf%20Compliance%20Editor%20v\d+.\d+.\d+.tar.gz\?[^"]+)</string>
				<key>url</key>
				<string>https://trusted.jamf.com/docs/establishing-compliance-baselines</string>
				<key>request_headers</key>
				<dict>
					<key>User-Agent</key>
					<string>Mozilla/5.0 (Macintosh; Intel Mac OS X 10_10_5) AppleWebKit/600.8.9 (KHTML, like Gecko) Version/8.0.8 Safari/600.8.9</string>
				</dict>
			</dict>
		</dict>
		<dict>
    			<key>Arguments</key>
    			<dict>
        			<key>string_to_replace</key>
        			<string>&amp;amp;</string>
        			<key>input_string</key>
        			<string>%match%</string>
        			<key>replacement_string</key>
        			<string>&amp;</string>
    			</dict>
    			<key>Processor</key>
   			<string>com.github.jelockwood.recipes.StringReplacer/StringReplacer</string>
		</dict>
		<dict>
    			<key>Arguments</key>
    			<dict>
        			<key>string_to_replace</key>
        			<string>//cdn.document360.io</string>
        			<key>input_string</key>
        			<string>%output_string%</string>
        			<key>replacement_string</key>
        			<string>https://cdn.document360.io</string>
    			</dict>
    			<key>Processor</key>
   			<string>com.github.jelockwood.recipes.StringReplacer/StringReplacer</string>
		</dict>
		<dict>
			<key>Processor</key>
			<string>URLDownloader</string>
			<key>Arguments</key>
			<dict>
				<key>url</key>
				<string>%output_string%</string>
			</dict>
		</dict>
		<dict>
			<key>Processor</key>
			<string>EndOfCheckPhase</string>
		</dict>
	</array>
</dict>
</plist>
